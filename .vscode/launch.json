{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "extensionHost",
            "request": "launch",
            "name": "Launch Extension Host",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceFolder}/vscode-ext"
            ],
            "env": {
                "PICOMET_BIN": "../target/debug/picomet",
                "RUST_BACKTRACE": "1"
            },
            "preLaunchTask": "ext:build"
        }
    ],
    "compounds": []
}