extern fn assert_eq(in actual: int, in expected: int);
extern fn print(in s: string);

fn main() {
    0 == 0 then {
        print("0 == 0\n");
    } else {
        print("0 != 0\n");
    };

    0 == 1 then {
        print("0 == 1\n");
    } else {
        print("0 != 1\n");
    };

    let x = 2;
    x == 1 then {
        print("0 == 1\n");
    } else if x == 2 then {
        print("x == 2\n");
    } else {
        print("x != 1, x != 2\n");
    };
}
