extern fn assert_eq(in actual: int, in expected: int);
extern fn print(in s: string);

fn main() {
    if 0 == 0 {
        print("0 == 0\n");
    } else {
        print("0 != 0\n");
    }

    if 0 == 1 {
        print("0 == 1\n");
    } else {
        print("0 != 1\n");
    }

    let x = 2;
    if x == 1 {
        print("0 == 1\n");
    } else if x == 2 {
        print("x == 2\n");
    } else {
        print("x != 1, x != 2\n");
    }
}
